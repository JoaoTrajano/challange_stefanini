generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model People {
  id        String   @id() @default(uuid())
  name     String
  gender   String?
  email String?
  birthDate DateTime
  birthplace String?
  nationality String?
  cpf String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("persons")
}
